{
  "name": "haganjones/laravel-observe-properties",
  "type": "library",
  "description": "A simple trait to allow you to observe changing properties on a Model.",
  "keywords": ["laravel", "trait", "observe"],
  "homepage": "https://github.com/haganjones/laravel-observe-properties",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "orchestra/testbench": "3.3.*",
    "mockery/mockery": "0.9.*",
    "orchestra/database": "3.3.*"
  },
  "autoload": {
    "psr-4": { "HaganJones\\LaravelObserveProperties\\": "app/Traits" },
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "authors": [
    {
      "name": "Sam Jones",
      "email": "sam@haganjones.com",
      "homepage": "https://haganjones.com",
      "role": "Developer"
    }
  ]
}
